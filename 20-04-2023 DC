#include <bits/stdc++.h>
 
using namespace std;

int main(int argc, char** argv)
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int mid=n/2;
    if(n%2!=0)
    {
        for(int i=mid-1;i<=mid+1;i++)
        {
            cout<<" "<<a[i];
        }
    }
    else
    {
        for(int i=mid-2;i<mid+2;i++)
        {
            cout<<" "<<a[i];
        }
    }
}
