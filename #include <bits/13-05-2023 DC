#include <bits/stdc++.h>
 
using namespace std;

int main(int argc, char** argv)
{
    int a,b,c;
    int flag=0;
    cin>>a>>b>>c;
    while(a>=c)
    {
        a=a-b;
        if(a==c)
        {
            flag=1;
        }
    }
    if(flag==1)
    {
        cout<<"YES";
    }
    else
    {
        cout<<"NO";
    }
}
