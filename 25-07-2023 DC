#include <bits/stdc++.h>
 
using namespace std;

int main(int argc, char** argv)
{
    int n,odd=0,even=0;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        int t;
        cin>>t;
        if(t%2==0)
        {
            even++;
        }
        else
        {
            odd++;
        }
    }
    if(odd>even)
    {
        cout<<"odd";
    }
    else if(even>odd)
    {
        cout<<"even";
    }
    else
    {
        cout<<"-1";
    }
}
