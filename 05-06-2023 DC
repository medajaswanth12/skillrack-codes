#include <bits/stdc++.h>
 
using namespace std;

int main(int argc, char** argv)
{
    char n[100];
    cin>>n;
    int len=strlen(n);
    for(int i=1;i<len;i+=2)
    {
        int res=(n[i-1]-'0')*(n[i]-'0');
        cout<<" "<<res;
    }
    if(len%2!=0)
    {
        cout<<" "<<n[len-1];
    }
    return 0;
}
